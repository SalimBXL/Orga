<tr id="log_<%= log.id %>">
    <td><%= log.date %></td>
    <td><%= log.adresse %></td>
    <td><%= log.utilisateur_id %></td>
    <td><%= log.description %></td>
    <td>
    <% if user_signed_in? && is_super_admin? %>
        <%= link_to '[edit]', edit_log_path(log) %>
        <%= link_to '[delete]', log_path(log),
            method: :delete,
            remote: true,
            data: { confirm: "Delete #{log.date} (#{log.description})  ?" }
        %>
    <% end %>
    </td>
</tr>